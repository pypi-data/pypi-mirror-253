# the default workflow class to load. Change this to load a custom workflow
FLASK_CLASSIC_WORKFLOW_CLASS=ml.workflows.inference.torch_inference_workflow.TorchInferenceWorkflow

# no pos args
FLASK_CLASSIC_WORKFLOW_POSITIONAL_ARGS=[]

# no kw args
FLASK_CLASSIC_WORKFLOW_KW_ARGS={"model_name":"YOUR/MODEL_HERE"}

# huggingface token
HUGGING_FACE_HUB_TOKEN=YOUR_TOKEN_HERE

# name of model file to load (this is already defaulted - shown here for completeness)
TORCH_MODEL_FILE_NAME=model.torch

# whether the model is a torch JIT file (as opposed to pickle) Currently only pickle supported.
USE_JIT=false
